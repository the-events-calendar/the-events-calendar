{
  "name": "the-events-calendar",
  "version": "7.0.0",
  "repository": "git@github.com:the-events-calendar/the-events-calendar.git",
  "_zipname": "the-events-calendar",
  "_zipfoldername": "the-events-calendar",
  "_resourcepath": "src/resources",
  "_domainPath": "lang",
  "_textDomain": "the-events-calendar",
  "_glotPressUrl": "https://translate.wordpress.org",
  "_glotPressSlug": "wp-plugins/the-events-calendar/stable",
  "_glotPressFileFormat": "%textdomain%-%wp_locale%.%format%",
  "_glotPressFormats": [
    "po",
    "mo"
  ],
  "_glotPressFilter": {
    "translation_sets": false,
    "minimum_percentage": 30,
    "waiting_strings": false
  },
  "_filePath": {
    "jest": [
      "src/modules/**/__tests__/**/*.js",
      "!src/resources/js/custom-tables-v1/**/__tests__/**/*.js",
      "tests/**/*.spec.js",
      "tests/**/*.spec.ts",
      "tests/**/*.spec.tsx"
    ]
  },
  "scripts": {
    "lint:js": "wp-scripts lint-js src/",
    "lint:css": "wp-scripts lint-style",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "format:css": "wp-scripts lint-style --fix",
    "format:js": "wp-scripts lint-js src/ --fix",
    "analyze": "wp-scripts build --webpack-bundle-analyzer",
    "build:common": "cd common && npm run build",
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "dev": "WP_DEVTOOL=eval-source-map wp-scripts start",
    "changelog": "./vendor/bin/changelogger add",
    "jest": "TZ=UTC gulp jest",
    "lint:eslint": "gulp eslint",
    "lint:stylelint": "gulp stylelint",
    "phpstan": "vendor/bin/phpstan analyse --configuration=phpstan.neon --level=0 --memory-limit=1G",
    "rebuild": "rm -rf node_modules && npm install && npm run build",
    "classy:format": "wp-scripts format src/resources/packages/classy tests/classy_jest && composer run classy:format",
    "classy:jest": "jest --config tests/classy_jest/jest.config.js tests/classy_jest",
    "classy:test": "npm run classy:jest && slic run classy_integration",
    "classy:pre-commit": "npm run classy:format && npm run classy:test",
    "test": "jest"
  },
  "devDependencies": {
    "@emotion/jest": "^11.13.0",
    "@jest/globals": "^29.7.0",
    "@stellarwp/tyson": "^0.0.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@wordpress/block-editor": "^14.15.0",
    "@wordpress/components": "^29.6.0",
    "@wordpress/compose": "^7.21.0",
    "@wordpress/data": "^10.19.0",
    "@wordpress/editor": "^14.19.0",
    "@wordpress/element": "^6.19.0",
    "@wordpress/hooks": "^1.3.3",
    "@wordpress/i18n": "^5.19.0",
    "@wordpress/plugins": "^7.19.0",
    "@wordpress/scripts": "^30.7.0",
    "cross-env": "^5.2.0",
    "enzyme-to-json": "^3.6.1",
    "file-loader": "^6.2.0",
    "gettext-parser": "1.3.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "postcss-custom-media": "^11.0.5",
    "postcss-import": "^16.1.0",
    "postcss-mixins": "^11.0.3",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.5",
