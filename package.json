    "@wordpress/is-shallow-equal": "^5.11.0",
    "@wordpress/url": "^4.11.0",
    "bezier-easing": "^2.1.0",
    "chrono-node": "^2.7.7",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.10",
    "date-fns": "^3.6.0",
