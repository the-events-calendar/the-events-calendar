{
  "name": "the-events-calendar",
  "version": "7.0.0",
  "repository": "git@github.com:the-events-calendar/the-events-calendar.git",
  "_zipname": "the-events-calendar",
  "_zipfoldername": "the-events-calendar",
  "_resourcepath": "src/resources",
  "_domainPath": "lang",
  "_textDomain": "the-events-calendar",
  "_glotPressUrl": "https://translate.wordpress.org",
  "_glotPressSlug": "wp-plugins/the-events-calendar/stable",
  "_glotPressFileFormat": "%textdomain%-%wp_locale%.%format%",
  "_glotPressFormats": ["po", "mo"],
  "_glotPressFilter": {
    "translation_sets": false,
    "minimum_percentage": 30,
    "waiting_strings": false
  },
  "_filePath": {
    "eslint": [
      "src/modules/**/*.js",
      "src/resources/js/**/*.js",
      "!src/resources/js/**/__tests__/**/*.js",
      "!src/resources/js/**/*.min.js",
      "!src/resources/js/app/**/*.js",
      "!src/resources/js/views/**/*.js",
      "!src/resources/js/events-admin.js",
      "!src/resources/js/tribe-events-bar.js",
      "!src/resources/js/tec-update-6.0.0-notice.js",
      "!src/resources/js/custom-tables-v1/ct1-upgrade.js",
      "!src/resources/js/tribe-events.js",
      "src/resources/js/views/**/*.js"
    ],
    "stylelint": [
      "src/resources/postcss/**/*.pcss",
      "!src/resources/postcss/aggregator-page.pcss",
      "!src/resources/postcss/events-admin.pcss",
      "!src/resources/postcss/freemius.pcss",
      "!src/resources/postcss/tribe-events-full-mobile.pcss",
      "!src/resources/postcss/tribe-events-full.pcss",
      "!src/resources/postcss/tribe-events-skeleton.pcss",
      "!src/resources/postcss/tribe-events-theme-mobile.pcss",
      "!src/resources/postcss/tribe-events-theme.pcss",
      "!src/resources/postcss/tribe-events-theme.pcss"
    ],
    "jest": [
      "src/modules/**/__tests__/**/*.js",
      "!src/resources/js/custom-tables-v1/**/__tests__/**/*.js"
    ]
  },
  "scripts": {
    "analyze": "webpack-bundle-analyzer -m static stats.json",
    "before-zip": "bash bin/action-before-zip.sh",
    "bootstrap": "./scripts/linkDependencies",
    "build:common": "npm run olp && cd common && npm run build",
    "build:gulp": "cross-env NODE_ENV=production gulp",
    "build:webpack": "npm run olp && cross-env NODE_ENV=production webpack -p",
    "build": "npm run olp && npm run build:webpack && npm run build:gulp",
    "changelog": "./vendor/bin/changelogger add",
    "dev": "npm run olp && cross-env NODE_ENV=development webpack -d --watch",
    "gettext-strings": "cross-env BABEL_ENV=gettext webpack",
    "glotpress": "gulp glotpress",
    "jest": "TZ=UTC gulp jest",
    "lint:eslint": "gulp eslint",
    "lint:stylelint": "gulp stylelint",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "olp": "export NODE_OPTIONS=--openssl-legacy-provider",
    "phpstan": "vendor/bin/phpstan analyse --configuration=phpstan.neon --level=0 --memory-limit=1G",
    "preanalyze": "cross-env NODE_ENV=production webpack -p --profile --json > stats.json",
    "rebuild": "rm -rf node_modules && npm install && npm run build"
  },
  "engines": { "node": "18.13.0", "npm": "8.19.3" },
  "devDependencies": {
    "@the-events-calendar/product-taskmaster": "^3.0.0",
    "@wordpress/hooks": "^1.3.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.6.1",
    "gettext-parser": "1.3.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-mock-store": "^1.5.3",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/runtime": "^7.15.3",
    "@moderntribe/common": "file:common/src/modules",
    "@moderntribe/events": "file:src/modules",
    "@nfen/redux-reducer-injector": "0.0.3",
    "@tanstack/react-query": "^4.29.12",
    "@wordpress/a11y": "1.0.3",
    "@wordpress/data": "^1.1.0",
    "@wordpress/i18n": "^1.2.2",
    "@wordpress/is-shallow-equal": "^1.1.4",
    "@wordpress/url": "1.0.0",
    "bezier-easing": "^2.1.0",
    "chrono-node": "^2.2.4",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.10",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "file-loader": "^1.1.6",
    "he": "^1.2.0",
    "intro.js": "^3.4.0",
    "jquery": "3.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mt-a11y-dialog": "^5.1.1",
    "prop-types": "^15.6.2",
    "querystringify": "^2.0.0",
    "react": "^16.14.0",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.14.0",
    "react-iframe": "^1.8.0",
    "react-input-autosize": "^2.2.1",
    "react-places-autocomplete": "^6.1.2",
    "react-redux": "^5.0.7",
    "react-scroll-to": "^1.2.2",
    "react-select": "^2.1.2",
    "react-text-input": "^0.0.8",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "uniqid": "^5.0.3",
    "url-loader": "^4.1.1",
    "validator": "^13.7.0",
    "whatwg-fetch": "^2.0.4",
    "zero-fill": "^2.2.3"
  },
  "overrides": { "babel-plugin-lodash": { "@babel/types": "~7.20.0" } }
}
